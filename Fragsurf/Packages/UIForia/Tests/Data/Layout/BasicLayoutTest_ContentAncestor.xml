<UITemplate>
    
    <Style>
        
        style container {
            FlexLayoutDirection = Horizontal;
        }
        
        style box {
            PreferredWidth = 100px;
        }
        
        
        
    </Style>
    
    <Contents style="container">
        
        <Div style="container">
            <Div attr:id="disable-me" style="box"/>
            <Div style="box"/>
        </Div>
        
    </Contents>
    
</UITemplate>