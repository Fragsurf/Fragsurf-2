<UITemplate>
    <Contents>

        <TemplateBindingTest_OnChange_Inner value="myValue" onChange:value="HandleChange()"/>
        
    </Contents>

    <Contents id="with_old_value">

        <TemplateBindingTest_OnChange_Inner value="myValue" onChange:value="HandleChange($oldValue)"/>

    </Contents>

    <Contents id="with_new_value">

        <TemplateBindingTest_OnChange_Inner value="myValue" onChange:value="HandleChange($newValue)"/>

    </Contents>

    <Contents id="with_sync">

        <TemplateBindingTest_OnChange_Inner sync:value="myValue" onChange:value="HandleChange($newValue, $oldValue)"/>

    </Contents>
    

</UITemplate>